/*
 Program: 2
Write a Java program to demonstrate the functionality of ‘super’ keyword at
1)	Variable level
2)	Method level
3)	Constructor level


 */

package pack21;

public class Son extends Father
{

	void SonM() 
	{
		System.out.println(super.wealth);
		   super.methF();
	}
    public Son()
    {
    	super();
    	new Son().SonM();
    }
	public static void main(String[] args) {
   
    Father aobj=new Father();
    
	}

}

class Father
{
	int wealth=500000;
	void methF()
	{
		System.out.println("method level");
	}
	public Father()
	{
		
		System.out.println("Constructor level");
	}
}
	