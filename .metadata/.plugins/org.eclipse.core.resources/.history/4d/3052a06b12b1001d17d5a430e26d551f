public class GetSumIndigits{
		
		public void getencrypted(String s)
			{
			    String str="";
			    int sum=0,k;
			    
			    
				
				for(int i=0;i<s.length();i++)
				{
					
					if(s.charAt(i)>='0' && s.charAt(i)<='9' )
					{
					   for(int j=0;j<s.length();j++)
					   {
